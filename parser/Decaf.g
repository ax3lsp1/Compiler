parser grammar Decaf;options {    tokenVocab=Decaf;}@header {package compiler.parser;}program		:	CLASS PROGRAM LLAVEI field_decl* method_decl* LLAVED;field_decl	:	type decl_list (COMA decl_list)* PUNTOCOMA;decl_list	:	ID | ID CORCHETEI int_literal CORCHETED;int_literal :	NUMEROS;method_decl	:	(type | VOID) ID PARENTESISI (type ID (COMA type ID)*)? PARENTESISD block;type 		: 	INT | BOOLEAN ;block : LLAVEI var_decl* statement* LLAVED;